<h1>Add a Car:</h1>

<%= simple_form_for @car do |f| %>
  <%= f.input :name %>
  <%= f.input :fuel %>
  <%= f.input :make %>
  <%= f.input :seats %>
  <%= f.file_field :photo %>
  <br>
  <br>
  <%= f.button :submit, class: 'btn btn-primary' %>
  <br>
  <br>
  <%= link_to 'Cancel', cars_path, class: 'btn btn-outline-danger' %>
<% end %>
